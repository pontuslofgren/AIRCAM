<!-- app/views/restaurants/_form.html.erb -->
<%= simple_form_for(@availability) do |f| %>
  <%= f.input :start_date,
    as: :string,
    input_html: { data: { controller: "datepicker" } } %>
  <%= f.input :end_date %>
  <%= f.submit 'Block dates', class:"btn btn-primary" %>
  <% # Button to go to camera#show when finished with adding dates %>
  <%= link_to 'Finished', camera_path(params[:id]), class:'btn btn-outline-primary' %>
<% end %>
