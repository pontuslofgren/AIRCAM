<div class="profile">
  <h1>My Profile</h1>

  <h4>First Name</h4>
  <p><%= current_user.first_name %></p>
  <h4>Last Name</h4>
  <p><%= current_user.last_name %></p>
  <h4>Email</h4>
  <p><%= current_user.email %></p>
  <h4>Phone Number</h4>
  <p><%= current_user.phone_number %></p>
  <h4>Address</h4>
  <p><%= current_user.address %></p>
  <h4>City</h4>
  <p><%= current_user.city %></p>
  <h4>Country</h4>
  <p><%= current_user.country %></p>
  <h4>Postal Code</h4>
  <p><%= current_user.postal_code %></p>
  <h4>Profile Picture</h4>
  <p><%= current_user.profile_picture %></p>
  <h4>Profile Description</h4>
  <p><%= current_user.profile_description %></p>
  <h4>Profile Rating</h4>
  <p><%= current_user.profile_rating %></p>
  <h4>Profile Reviews</h4>
  <p><%= current_user.profile_reviews %></p>
  <h4>Profile Cameras</h4>
  <p><%= current_user.profile_cameras %></p>

